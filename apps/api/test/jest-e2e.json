{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/shared-services": "<rootDir>/../libs/shared-services/src",
    "@app/shared-services/(.*)": "<rootDir>/../libs/shared-services/src/$1"
  }
}